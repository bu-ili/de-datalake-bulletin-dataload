{
    "base_url": "https://www.bu.edu/academics/wp-json/wp/v2/",
    "endpoints": {
        "pages": "pages?",
        "media": "media?"
    },
    "initial_pagination_param": "?per_page=100&page=",
    "loop_pagination_param": "&per_page=100&page=",
    "default_baseline_datetime": "2000-01-01T00:00:00",
    "idempotency_param": "&modified_after=",
    "partition_date_prefix": "load_date=",
    "partition_time_prefix": "load_time=",
    "http_client": {
        "max_connections": 50,
        "max_keepalive_connections": 20,
        "connect_timeout": 10.0,
        "total_timeout": 30.0,
        "http2": true
    },
    "paths": {
        "parquet_export_folder_path": "/bulletin_raw/",
        "parquet_file_name": "de_bulletin_data.parquet",
        "parquet_compression": "SNAPPY"
    },
    "aws": {
        "s3_bucket_name": "buaws-datalake-nonprod-s3",
        "region_name": "us-east-1"
    }
}